dot = Digraph()
for node in map.nodes:
    dot.node(node)

for edge in map.arcs:
    dot.edge(edge.source, edge.target, label=str(edge.weight))

# Display the heuristic process map using Graphviz in Streamlit
st.graphviz_chart(dot.source)
